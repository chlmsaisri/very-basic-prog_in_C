//O(n)

#include <stdio.h>
#include <string.h>

int main() {
    char s1[] = "Hello, WORLD!";
    int i, l;

    l = strlen(s1);
    for (i = 0; i < l; i++) {
        if (s1[i] >= 'A' && s1[i] <= 'Z') {
            printf("%c", s1[i] + 32);
        } else {
            printf("%c", s1[i]);
        }
    }
    
    printf("\n");

    return 0;
}
