#include <stdio.h>
#include <string.h>

int main() {
    char s[] = "Hello, world!";
    int length = strlen(s);

    printf("String: %s\n", s);
    printf("Length: %d\n", length);

    return 0;
}



-----------


#include <stdio.h>

int main() {
    char str[] = "Hello, world!";
    int length = 0;

    for (int i = 0; str[i] != '\0'; i++) {
        length++;
    }

    printf("Length: %d\n", length);

    return 0;
}
